{
  "name": "svelte-remixicon",
  "description": "An icon library for Svelte/SvelteKit based on Remix Icon. Consists of more than 3000 icons.",
  "version": "2.9.1",
  "license": "MIT",
  "main": "dist/index.js",
  "author": {
    "name": "Antu Saha",
    "email": "imantu09@gmail.com",
    "url": "https://github.com/aantusahaa"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aantusahaa/svelte-remixicon"
  },
  "keywords": [
    "svelte",
    "sveltekit",
    "remixicon",
    "svg icon",
    "icon library",
    "icon"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "node build && svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./*.svelte": {
      "types": "./dist/icons/*.svelte.d.ts",
      "svelte": "./dist/icons/*.svelte"
    }
  },
  "typesVersions": {
    ">4.0": {
      ".": [
        "./dist/index.d.ts"
      ],
      "*.svelte": [
        "./dist/icons/*.svelte.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^3.54.0 || ^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/kit": "^2.46.4",
    "@sveltejs/package": "^2.5.4",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "pascal-case": "^3.1.2",
    "publint": "^0.1.16",
    "svelte": "^4.2.20",
    "svelte-check": "^3.8.6",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^5.4.20"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "packageManager": "pnpm@9.3.0+sha512.ee7b93e0c2bd11409c6424f92b866f31d3ea1bef5fbe47d3c7500cdc3c9668833d2e55681ad66df5b640c61fa9dc25d546efa54d76d7f8bf54b13614ac293631"
}
